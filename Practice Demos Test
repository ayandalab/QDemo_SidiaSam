    "def duplicates(A):\n",
    "\treturn list(dict.fromkeys(A))\n",
    "\n",
    "def getFullSum(A):\n",
    "\tMaximum = max(A)\n",
    "\tsum = 0\n",
    "\tfor i in range(Maximum+1):\n",
    "\t\tsum = sum + i\n",
    "\treturn sum\n",
    "\n",
    "def sumOfArr(A):\n",
    "\tif sum(A) <= 0:\n",
    "\t\treturn -1\n",
    "\treturn sum(A)\n",
    "\n",
    "def solution(A):\n",
    "\tA = duplicates(A)\n",
    "\tFSum = getFullSum(A)\n",
    "\tSum = sumOfArr(A)\n",
    "\treturn FSum - Sum"
